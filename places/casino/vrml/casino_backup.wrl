PROTO PopUp[
eventIn SFTime set_pop
eventIn SFString new_url
exposedField SFString url ""
exposedField SFString status "yes"
exposedField SFString scrollbars "auto"
exposedField SFInt32  height 300
exposedField SFInt32  width 200
exposedField SFString description "Pop Up Info"
exposedField MFNode children []
eventOut SFBool isOver
eventOut SFTime touchTime
]{
Group{children[
DEF button TouchSensor{isOver IS isOver touchTime IS touchTime}
Group{children IS children}
DEF pop Script{
eventIn SFTime set_pop
eventIn SFBool set_over
eventIn SFString new_url IS new_url
field SFString linkURL IS url
field SFString status IS status
field SFString scrollbars IS scrollbars
field SFInt32  height IS height
field SFInt32  width IS width
field SFString description IS description
field MFString param ["","","target=_self"]
url"vrmlscript:
function new_url(v,t){
 linkURL = v;
}
function set_pop(v,t){
 st = new MFString('OnEvent(status,'+status+')');
 sc = new MFString('OnEvent(scrollbars,'+scrollbars+')');
 h = new MFString('OnEvent(height,'+height+')');
 w = new MFString('OnEvent(width,'+width+')');
 u = new MFString('OnEvent(load,'+linkURL+')');
 Browser.loadURL(st,param);
 Browser.loadURL(sc,param); 
 Browser.loadURL(h,param);
 Browser.loadURL(w,param);
 Browser.loadURL(u,param);
 Browser.setDescription('');
}
function set_over(v,t){
 if(v){ Browser.setDescription(description);}
 else{Browser.setDescription('');}
}
"}
]}
ROUTE button.touchTime TO pop.set_pop
ROUTE button.isOver TO pop.set_over
}#END PopUp PROTO

PROTO Televator [
eventIn SFFloat set_time
field MFFloat upkey []
field MFVec3f uppath []
field SFVec3f upposition 0 0 0
field MFFloat downkey []
field MFVec3f downpath []
field SFVec3f downposition 0 0 0
field SFRotation orientation 0 1 0 0
field SFVec3f size 1 1 1
eventOut SFTime triggerTime
]{
Group{
children[
DEF utProx ProximitySensor{ enterTime IS triggerTime size IS size center IS upposition}
DEF utInterp PositionInterpolator{key IS upkey keyValue IS uppath}
DEF utClock TimeSensor{cycleInterval 1 loop FALSE enabled TRUE}
DEF dtProx ProximitySensor{enterTime IS triggerTime size IS size center IS downposition}
DEF dtInterp PositionInterpolator{key IS downkey keyValue IS downpath}
DEF dtClock TimeSensor{cycleInterval 1 loop FALSE enabled TRUE}
DEF t Transform{children[
DEF tView Viewpoint{orientation IS orientation jump TRUE}
]}#END t


DEF ts Script{
eventIn SFBool set_uptrigger
eventIn SFBool set_downtrigger

eventIn SFBool set_up
eventIn SFBool set_down

field SFBool isUsed FALSE
eventOut SFBool bind_changed
eventOut SFTime upTime_changed
eventOut SFTime downTime_changed
url"vrmlscript:

function set_uptrigger(v,t){
 if(!v || isUsed){return;}
 else{
  bind_changed = true;
  upTime_changed = t;
  isUsed = true;
 }
}

function set_downtrigger(v,t){
 if(!v || isUsed){return;}
 else{
  bind_changed = true;
  downTime_changed = t;
  isUsed = true;
 }
}

function set_up(v){
 if(v){return;}
 else{isUsed = false;}
}

function set_down(v){
 if(v){return;}
 else{isUsed = false;}
}

"}
]}
ROUTE utProx.isActive TO ts.set_uptrigger
ROUTE ts.upTime_changed TO utClock.set_startTime
ROUTE utClock.fraction_changed TO utInterp.set_fraction
ROUTE utInterp.value_changed TO t.set_translation
ROUTE dtProx.isActive TO ts.set_downtrigger
ROUTE ts.downTime_changed TO dtClock.set_startTime
ROUTE dtClock.fraction_changed TO dtInterp.set_fraction
ROUTE dtInterp.value_changed TO t.set_translation
ROUTE utClock.isActive TO ts.set_up
ROUTE dtClock.isActive TO ts.set_down
ROUTE ts.bind_changed TO tView.set_bind
}#END televator PROTO

