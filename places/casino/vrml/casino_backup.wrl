DEF evaluateGame Script {
eventIn SFBool check
eventOut SFBool win
eventOut SFBool enableHebel
eventOut SFInt32 points IS points
field SFNode node_L USE timer_L
field SFNode node_M USE timer_M
field SFNode node_R USE timer_R
field SFNode node_Blink USE timer_Blink
field SFNode RolleL USE Rolle_L_16
field SFNode RolleM USE Rolle_M_18
field SFNode RolleR USE Rolle_R_20
field SFNode xvalue USE Values
field SFNode xanzeige USE Anzeige_Text_25
field SFNode bet_L USE Bet_Button_L_Col
field SFNode bet_M USE Bet_Button_M_Col
field SFNode bet_R USE Bet_Button_R_Col
field SFNode bet_anzeige USE Anzeige_Text_Bet
field SFNode dspl USE WIN_DISPLAY
field SFNode alpha USE Alphabet
field SFInt32 Value_L 0
field SFInt32 Value_M 0
field SFInt32 Value_R 0
field SFInt32 i 0
field SFInt32 j 0
url "vrmlscript: 
function check(value, stamp) 
{ 
if (!node_L.isActive_changed && !node_M.isActive_changed && !node_R.isActive_changed) 
 { 
 for (i=5; i >= -1; i--) 
 { 
 if (RolleL.rotation[3] > i* 0.5236) 
 break; 
 } 
 Value_L = i+1; 
 if (RolleL.rotation[0] == 1) 
 Value_L = (11-i); 
 
 for (i=5; i >= -1; i--) 
 { 
 if (RolleM.rotation[3] > i* 0.5236) 
 break; 
 } 
 Value_M = i+1; 
 if (RolleM.rotation[0] == 1) 
 Value_M =( 11-i); 
 
 for (i=5; i >= -1; i--) 
 { 
 if (RolleR.rotation[3] > i* 0.5236) 
 break; 
 } 
 Value_R = i+1; 
 if (RolleR.rotation[0] == 1) 
 Value_R = (11-i); 
 
 
 
 if (Value_L == 11) 
 { 
 if (Value_M == 11) 
 { 
 if (Value_R == 11) 
 { 
 Value_L = 0; 
 Value_M = 0; 
 Value_R = 0; 
 } 
 else 
 { 
 Value_L = Value_R; 
 Value_M = Value_R; 
 } 
 } 
 else 
 if (Value_R == 11) 
 { 
 Value_L = Value_M; 
 Value_R = Value_M; 
 } 
 else 
 if (Value_R == 0) 
 Value_L = Value_R; 
 else 
 Value_L = Value_M; 
 } 
 else 
 if (Value_M == 11) 
 { 
 if (Value_R == 11) 
 { 
 Value_M = Value_L; 
 Value_R = Value_L; 
 } 
 else 
 { 
 if ((Value_L == 0) || (Value_R == 0)) 
 Value_M = 0; 
 else 
 Value_M = Value_L; 
 } 
 } 
 else 
 if (Value_R == 11) 
 Value_R = Value_M; 
 
 
 // case 3 equal 
 // nnn 
 if ((Value_L == Value_M) && 
 (Value_M == Value_R)) 
 { 
 if (Value_L == 0) 
 xvalue.rotation[0] += (xvalue.rotation[1] * 10); 
 else 
 xvalue.rotation[0] += (xvalue.rotation[1] * 5); 
 
 if (xvalue.rotation[0] > xvalue.translation[0]){ 
 points                  = xvalue.rotation[0]; 
 xvalue.translation[0]   = xvalue.rotation[0]; 
 } 
 
 node_Blink.startTime = stamp; 
 node_Blink.stopTime = stamp + 2; 
 node_Blink.set_enabled = TRUE; 
 } 
 else 
 // case 2 equal 
 // nnx nxn xnn 
 if ((Value_L == Value_M) || 
 (Value_L == Value_R) || 
 (Value_M == Value_R)) 
 { 
 if ((Value_L == 0 && Value_M == 0) || 
 (Value_L == 0 && Value_R == 0) || 
 (Value_M == 0 && Value_R == 0)) 
 xvalue.rotation[0] += (xvalue.rotation[1]*2); 
 else 
 xvalue.rotation[0] += xvalue.rotation[1]; 
 
 if (xvalue.rotation[0] > xvalue.translation[0]){ 
 points                  = xvalue.rotation[0]; 
 xvalue.translation[0]   = xvalue.rotation[0]; 
 } 
 
 node_Blink.startTime = stamp; 
 node_Blink.stopTime = stamp + 2; 
 node_Blink.set_enabled = TRUE; 
 } 
 else 
 { 
 node_Blink.set_enabled = FALSE; 
 xvalue.rotation[0] -= xvalue.rotation[1]; 
 
//##################################naechste Zeile loeschen !!!########################### 
 
 //     points = xvalue.rotation[0]; 
 
 } 
 // no more money 
 if (xvalue.rotation[0] <= 0) 
 { 
 // reset bet button to bet NULL 
 bet_L.diffuseColor[1] = 1; 
 bet_L.diffuseColor[2] = 1; 
 
 bet_M.diffuseColor[1] = 1; 
 bet_M.diffuseColor[2] = 1; 
 
 bet_R.diffuseColor[1] = 1; 
 bet_R.diffuseColor[2] = 1; 
 
 // set new value 
 xvalue.rotation[0] = 0; 
 
 xvalue.rotation[1] = 0; 
 bet_anzeige.string[0] = 'Bet: None'; 
 } 
 else 
 // bet ALL or bet bigger than score 
 if (xvalue.rotation[1] > xvalue.rotation[0]) 
 { 
 // reset bet button to bet NULL 
 bet_L.diffuseColor[1] = 1; 
 bet_L.diffuseColor[2] = 1; 
 
 bet_M.diffuseColor[1] = 1; 
 bet_M.diffuseColor[2] = 1; 
 
 bet_R.diffuseColor[1] = 1; 
 bet_R.diffuseColor[2] = 1; 
 
 // is it possible to bet 50 ? 
 if (xvalue.rotation[0] >= 50) 
 { 
 xvalue.rotation[1] = 50; 
 bet_M.diffuseColor[1] = 0; 
 bet_M.diffuseColor[2] = 0; 
 bet_anzeige.string[0] = 'Bet: 50'; 
 } 
 else 
 // set bet to 10 
 { 
 xvalue.rotation[1] = 10; 
 bet_L.diffuseColor[1] = 0; 
 bet_L.diffuseColor[2] = 0; 
 bet_anzeige.string[0] = 'Bet: 10'; 
 } 
 } 
 
 // Gewinnanzeige 
 
 Value_L = xvalue.rotation[0]; 
 for (i=4; i >= 0; i--) 
 { 
 j = Value_L % 10; 
 Value_L /= 10; 
 
 dspl.point[i*4+0] = alpha.point[j*4+64]; 
 dspl.point[i*4+1] = alpha.point[j*4+65]; 
 dspl.point[i*4+2] = alpha.point[j*4+66]; 
 dspl.point[i*4+3] = alpha.point[j*4+67]; 
 } 
 
 
//      xanzeige.string[0] = xvalue.rotation[0] + ''; 
 // free lock 
 enableHebel = TRUE; 
 } 
else 
 { 
 node_Blink.set_enabled = FALSE; 
 enableHebel = FALSE; 
 } 
}"
}
