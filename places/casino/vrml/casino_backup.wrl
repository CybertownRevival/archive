DEF SetScoreScript Script {
eventIn SFString highscorelist IS highscorelist
eventOut MFString theList
field SFNode xvalue USE Values
url "vrmlscript: 
 function  highscorelist(value) { 
 if (value.length <= 8){ 
 //value in int umwandeln 
 temp = new SFString(value); 
 pos=0; 
 sum = 0; 
 for (i=0; i<value.length; i++){ 
 ch = temp.substring(pos,pos+1); 
//Browser.print(ch); 
 if (ch == '0') 
 sum *= 10; 
 else if (ch == '1') 
 sum = sum*10 + 1; 
 else if (ch == '2') 
 sum = sum*10 + 2; 
 else if (ch == '3') 
 sum = sum*10 + 3; 
 else if (ch == '4') 
 sum = sum*10 + 4; 
 else if (ch == '5') 
 sum = sum*10 + 5; 
 else if (ch == '6') 
 sum = sum*10 + 6; 
 else if (ch == '7') 
 sum = sum*10 + 7; 
 else if (ch == '8') 
 sum = sum*10 + 8; 
 else if (ch == '9') 
 sum = sum*10 + 9;                                        
 pos++; 
 } 
 xvalue.translation[0] = sum; 
 } 
 else { 
 pos1=0; 
 aList = new MFString(); 
 temp = new SFString(value); 
 pos2 = temp.indexOf('§',pos1);  
 temp2 = new SFString(temp.substring(pos1,pos2)); 
 if ( temp2 == 'error' ){ 
 temp2 = new SFString(temp.substring(pos2+1,temp.length));        
 Browser.setDescription('error: '+temp2); 
 } 
 else { 
 for (i=0; i<5;i++){ 
 pos2 = temp.indexOf('§',pos1); 
 aList[i] = new SFString(temp.substring(pos1, pos2)); 
 pos1 = pos2+1; 
 pos2 = temp.indexOf('§',pos1); 
 aList[i] += '  '; 
 aList[i] += temp.substring(pos1, pos2); 
 pos1 = pos2+1;  
 } 
 theList = aList; 
 } 
 } 
 }"
}
