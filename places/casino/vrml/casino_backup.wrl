DEF loader Script{
eventIn MFNode receive
field MFString loadURL "/cgi-bin/games2/ec/cgamesload.pl"
field MFNode games [
USE cg0,
USE cg1,
USE cg2,
USE cg3,
USE cg4,
]
field SFNode self USE loader 
field MFNode empty []
url"vrmlscript:
function receive(v,t){
 if(v == empty){return;}
 for(i=0;i < v[0].name.length;i++){
  games[i].name = new MFString(v[0].name[i]);
  games[i].new_url = new SFString(v[0].url[i]);
 }
}
function initialize(){Browser.createVrmlFromURL(loadURL,self,'receive');}
"}
